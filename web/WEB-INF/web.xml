<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
		 
		 
	<servlet>
		<servlet-name>business</servlet-name>
		<servlet-class>controller.BusinessServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>business</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>business</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>business</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>business</servlet-name>
		<url-pattern>/navbar</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>business</servlet-name>
		<url-pattern>/businessList</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>business</servlet-name>
		<url-pattern>/businessForm</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>business</servlet-name>
		<url-pattern>/adduser</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>business</servlet-name>
		<url-pattern>/deleteBusiness</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>business</servlet-name>
		<url-pattern>/editBusiness</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>business</servlet-name>
		<url-pattern>/updateBusiness</url-pattern>
	</servlet-mapping>



	<servlet>
		<servlet-name>question</servlet-name>
		<servlet-class>controller.QuestionServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>question</servlet-name>
		<url-pattern>/questionList</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>question</servlet-name>
		<url-pattern>/questionForm</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>question</servlet-name>
		<url-pattern>/addQuestion</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>question</servlet-name>
		<url-pattern>/deleteQuestion</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>question</servlet-name>
		<url-pattern>/editQuestion</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>question</servlet-name>
		<url-pattern>/updateQuestion</url-pattern>
	</servlet-mapping>

		 
		 
		 
		 
</web-app>